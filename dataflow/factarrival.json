{
	"name": "factarrival",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     PID as string (\"0\")",
				"}",
				"source(output(",
				"          invoiceId as string,",
				"          customerid as string,",
				"          invoiceDate as string,",
				"          ShipTo as string,",
				"          ShipTo_City as string,",
				"          ShipTo_Country as string,",
				"          SalespersonPersonID as string,",
				"          SalespersonName as string,",
				"          total_product_cnt as string,",
				"          Total as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source1 keyGenerate(output(invoice_sk as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(invoice_sk = invoice_sk,",
				"          DI_CREATED_DT = currentTimestamp(),",
				"          PID = $PID) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     mapColumn(",
				"          INVOICE_SK = invoice_sk,",
				"          INVOICE_ID = invoiceId,",
				"          CUSTOMER_ID = customerid,",
				"          INVOICE_DT = invoiceDate,",
				"          TOTAL_PRODUCT_CNT = total_product_cnt,",
				"          TOTAL_PRODUCT_COST = Total,",
				"          DI_CREATED_DT,",
				"          PID",
				"     ),",
				"     format: 'table') ~> sink1"
			]
		}
	}
}